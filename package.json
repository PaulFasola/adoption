{
  "name": "adoption",
  "license": "MIT",
  "version": "0.0.0",
  "description": "",
  "author": "Paul Fasola",
  "private": true,
  "keywords": [
    "bsc",
    "eth",
    "dApp",
    "UI",
    "components"
  ],
  "repository": "https://github.com/PaulFasola/react-multiwallet-connector",
  "homepage": "https://github.com/PaulFasola/react-multiwallet-connector#readme",
  "bugs": {
    "url": "https://github.com/PaulFasola/react-multiwallet-connector/issues"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "dev": "npm run storybook:start",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "lint": "npm run lint:script",
    "lint:script": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "storybook:start": "start-storybook -p 9001 -s ./src/assets -c .storybook",
    "storybook:build": "build-storybook -o doc",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm run test --watch",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "publish": "npx semantic-release --no-ci"
  },
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/react": "^6.1.20",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.2.2",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^4.42.1"
  },
  "engines": {
    "node": ">=12.0.0",
    "yarn": "^1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "preset": "conventionalcommits",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [skip ci]",
          "assets": [
            "CHANGELOG"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "styled-components": "^5.2.1"
  }
}